/* uni.scss */
.tool-content-box {
  width: 150rpx;
  height: 150rpx;
  border-radius: 10rpx;
  overflow: hidden;
  border: 1px solid #EEEEEE;
}
.tool-box {
  width: 150rpx;
  height: 150rpx;
  border: 1px dashed #000000;
  border-radius: 10rpx;
}
.tool-image {
  width: 100rpx;
  height: 100rpx;
}
.tool-cover-mini {
  width: 100%;
  height: 100%;
}
.tool-close-icon {
  width: 35rpx;
  height: 35rpx;
}
.tool-player-icon {
  width: 70rpx;
  height: 70rpx;
}
.tool-upload-fail {
  background-color: #FA3534;
  color: #FFFFFF;
}

